{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Out",
    "definitions": {
        "Out": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "persons": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Person"
                    }
                }
            },
            "required": [
                "persons"
            ],
            "title": "Out"
        },
        "Person": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "surname": {
                    "type": "string"
                },
                "age": {
                    "type": "integer"
                },
                "phone": {
                    "type": "string"
                },
                "test": {
                    "type": "string",
                    "format": "integer"
                }
            },
            "required": [
                "age",
                "name",
                "phone",
                "surname"
            ],
            "title": "Person"
        }
    }
}
