{
    "xsd:schema": {
        "@xmlns:xsd": "http://www.w3.org/2001/XMLSchema",
        "#": [
            {
                "xsd:element": {
                    "@name": "statusAddress",
                    "@type": "complexType5"
                }
            },
            {
                "xsd:simpleType": [
                    {
                        "@name": "dateType",
                        "xsd:union": {
                            "xsd:simpleType": [
                                {
                                    "xsd:restriction": {
                                        "@base": "xsd:date"
                                    }
                                },
                                {
                                    "xsd:restriction": {
                                        "@base": "xsd:string",
                                        "xsd:pattern": {
                                            "@value": "(0?[1-9]|[12][0-9]|3[01])[/.](0?[1-9]|1[0-2])[/.]\\\\d{4}"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "@name": "timeType",
                        "xsd:union": {
                            "xsd:simpleType": [
                                {
                                    "xsd:restriction": {
                                        "@base": "xsd:time"
                                    }
                                },
                                {
                                    "xsd:restriction": {
                                        "@base": "xsd:string",
                                        "xsd:pattern": {
                                            "@value": "([0-1]?[0-9]|2[0-3]):([0-5][0-9])"
                                        }
                                    }
                                },
                                {
                                    "xsd:restriction": {
                                        "@base": "xsd:string",
                                        "xsd:pattern": {
                                            "@value": "(0?[0-9]|1[01]):([0-5][0-9]) (AM|PM|a\\\\.m\\\\.|p\\\\.m\\\\.)"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "@name": "booleanStringType",
                        "xsd:restriction": {
                            "@base": "xsd:string",
                            "xsd:pattern": {
                                "@value": "true|false"
                            }
                        }
                    },
                    {
                        "@name": "integerStringType",
                        "xsd:restriction": {
                            "@base": "xsd:string",
                            "xsd:pattern": {
                                "@value": "(0|-?[1-9]*)"
                            }
                        }
                    },
                    {
                        "@name": "uriType",
                        "xsd:restriction": {
                            "@base": "xsd:string",
                            "xsd:pattern": {
                                "@value": "(https?|ftp):\\\\/\\\\/[^{}]+\\\\.[^{}]+"
                            }
                        }
                    },
                    {
                        "@name": "nullType",
                        "xsd:restriction": {
                            "@base": "xsd:string",
                            "xsd:length": {
                                "@value": "0"
                            }
                        }
                    }
                ]
            },
            {
                "xsd:complexType": [
                    {
                        "@name": "complexType1",
                        "xsd:all": {
                            "xsd:element": [
                                {
                                    "@name": "persons",
                                    "@type": "complexType2"
                                },
                                {
                                    "@name": "address",
                                    "@type": "complexType4"
                                }
                            ]
                        }
                    },
                    {
                        "@name": "complexType2",
                        "xsd:sequence": {
                            "xsd:element": {
                                "@maxOccurs": "unbounded",
                                "@minOccurs": "0",
                                "@name": "personsItem",
                                "@type": "complexType3"
                            }
                        }
                    },
                    {
                        "@name": "complexType3",
                        "xsd:all": {
                            "xsd:element": [
                                {
                                    "@name": "name",
                                    "@type": "xsd:string"
                                },
                                {
                                    "@name": "surname",
                                    "@type": "xsd:string"
                                },
                                {
                                    "@name": "age",
                                    "@type": "xsd:integer"
                                },
                                {
                                    "@name": "birthday",
                                    "@type": "dateType"
                                },
                                {
                                    "@name": "phone",
                                    "@type": "xsd:string"
                                },
                                {
                                    "@name": "status",
                                    "@type": "complexType4"
                                },
                                {
                                    "@name": "address",
                                    "@type": "complexType5"
                                },
                                {
                                    "@minOccurs": "0",
                                    "@name": "driver",
                                    "@type": "xsd:boolean"
                                }
                            ]
                        }
                    },
                    {
                        "@name": "complexType4",
                        "xsd:all": {
                            "xsd:element": [
                                {
                                    "@name": "activity",
                                    "@type": "xsd:string"
                                },
                                {
                                    "@name": "organizationName",
                                    "@type": "xsd:string"
                                },
                                {
                                    "@name": "address",
                                    "@type": "complexType5"
                                }
                            ]
                        }
                    },
                    {
                        "@name": "complexType5",
                        "xsd:all": {
                            "xsd:element": [
                                {
                                    "@name": "street",
                                    "@type": "xsd:string"
                                },
                                {
                                    "@name": "city",
                                    "@type": "xsd:string"
                                },
                                {
                                    "@name": "state",
                                    "@type": "xsd:string"
                                },
                                {
                                    "@name": "postalCode",
                                    "@type": "xsd:string"
                                }
                            ]
                        }
                    }
                ]
            },
            {
                "xsd:element": [
                    {
                        "@name": "root",
                        "@type": "complexType1"
                    },
                    {
                        "@name": "persons",
                        "@type": "complexType2"
                    },
                    {
                        "@name": "status",
                        "@type": "complexType4"
                    },
                    {
                        "@name": "rootAddress",
                        "@type": "complexType4"
                    }
                ]
            }
        ]
    }
}s